{% autoescape off %}
{
    "id" : "{{project.id}}",
    "name" : "{{project.name}}",
    "status" : "{{project.status}}",
    "created_by" : "{{project.created_by}}",
    {% if publishable %}
        "publishable" : true,
    {% else %}
        "publishable" : false,
    {% endif %}
    "authorized" : true
}
{% endautoescape %}