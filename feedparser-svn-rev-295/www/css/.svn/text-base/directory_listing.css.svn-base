h2 {
  display: none;
}

table {
  border-collapse: collapse;
  border-spacing: 0px 0px 5px 0px;
  margin-top: 1em;
  width: 96%;
}

td, th {
  font-family: monospace;
  font-size: medium !important;
  font-size: small;
}

html>body table th {
  text-align: right;
  padding-right: 10px;
}

html>body table th:first-child {
  text-align: left;
  padding-left: 20px;
}

td {
  border-bottom: 1px solid silver;
  padding: 3px 10px 3px 20px;
}

html>body td {
  border-bottom: 1px dotted #003399;
}

th {
  padding-bottom: 1em;
}

tr {
  padding-bottom: 5px;
}

td a:link, td a:visited, td a:hover {
  border-bottom: 0;
}

tr.ruled {
  background-color: #ffeecc ! important;
}
